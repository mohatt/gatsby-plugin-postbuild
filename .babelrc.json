{
  "presets": [
    [
      "babel-preset-gatsby-package",
      {
        "browser": false,
        "nodeVersion": "10.13.0"
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "~": "./src",
          "#": "./test"
        }
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-proposal-private-methods",
      {
        "loose": true
      }
    ]
  ]
}
