{
  "name": "gatsby-plugin-postbuild",
  "version": "1.2.2",
  "description": "Gatsby plugin for optimizing HTML/CSS files after build",
  "files": [
    "dist",
    "index.js",
    "gatsby-node.js"
  ],
  "scripts": {
    "prepare": "cross-env NODE_ENV=production npm run build",
    "build": "babel src --out-dir dist -x '.ts,.js' --delete-dir-on-start --ignore '**/__tests__,**/*.test.js' -s",
    "watch": "babel -w src --out-dir dist -x '.ts,.js' --delete-dir-on-start --ignore '**/__tests__,**/*.test.js'",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --watch --runInBand",
    "release": "semantic-release",
    "release:inspect": "semantic-release --dry-run",
    "release:manual": "semantic-release --no-ci",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:config": "eslint --print-config .eslintrc.json"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "optimization",
    "purgecss",
    "tailwindcss"
  ],
  "author": "Mohamed Elkholy <mohatt@pm.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohatt/gatsby-plugin-postbuild.git"
  },
  "homepage": "https://github.com/mohatt/gatsby-plugin-postbuild",
  "bugs": "https://github.com/mohatt/gatsby-plugin-postbuild/issues",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "bluebird": "^3.7.2",
    "debug": "^3.2.6",
    "filesize": "^6.1.0",
    "lodash": "^4.17.20",
    "parse5": "^6.0.0",
    "parse5-htmlparser2-tree-adapter": "^6.0.0",
    "purgecss": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.17",
    "@babel/core": "7.12.17",
    "@babel/eslint-parser": "7.12.17",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-private-methods": "7.12.13",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "@types/jest": "26.0.20",
    "@types/bluebird": "3.5.33",
    "@types/lodash": "4.14.168",
    "@types/parse5": "6.0.0",
    "@types/parse5-htmlparser2-tree-adapter": "6.0.0",
    "babel-preset-gatsby-package": "0.12.0",
    "cross-env": "7.0.3",
    "eslint": "7.20.0",
    "eslint-config-standard-with-typescript": "20.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "gatsby": "2.32.4",
    "gatsby-plugin-utils": "0.9.0",
    "jest": "26.6.3",
    "metro-memory-fs": "0.65.1",
    "mkdirp": "1.0.4",
    "semantic-release": "17.3.9",
    "typescript": "4.1.5"
  },
  "peerDependencies": {
    "gatsby": ">=2.25.0"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/test/",
      "<rootDir>/src/"
    ],
    "collectCoverageFrom": [
      "src/**/*.(js|ts)"
    ]
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "renovate": {
    "extends": [
      "config:js-lib"
    ]
  }
}
