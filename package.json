{
  "name": "gatsby-plugin-postbuild",
  "version": "3.0.2",
  "description": "Gatsby plugin for optimizing/transforming generated static files.",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./gatsby-node": {
      "types": "./dist/gatsby-node.d.ts",
      "require": "./dist/gatsby-node.cjs",
      "import": "./dist/gatsby-node.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "gatsby-node.js"
  ],
  "scripts": {
    "build": "pkgroll --clean-dist --sourcemap --target node18 --env.VITEST=false",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "prepare": "NODE_ENV=production pnpm build",
    "release": "semantic-release --no-ci",
    "release:inspect": "semantic-release --dry-run",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc -p . --noEmit"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "postbuild",
    "optimization",
    "transformation"
  ],
  "author": "Mohamed Elkholy <mohatt@pm.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohatt/gatsby-plugin-postbuild.git"
  },
  "homepage": "https://github.com/mohatt/gatsby-plugin-postbuild",
  "bugs": "https://github.com/mohatt/gatsby-plugin-postbuild/issues",
  "dependencies": {
    "@types/glob": "^7.1.3",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.1",
    "cssnano": "^7.1.0",
    "debug": "^4.4.0",
    "gatsby-plugin-utils": "^4.10.0",
    "filesize": "^8.0.0",
    "glob": "^7.1.6",
    "lodash": "^4.17.21",
    "parse5": "^7.3.0",
    "purgecss": "^6.0.0",
    "terser": "^5.43.1",
    "webpack-assets-manifest": "^6.2.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/bluebird": "3.5.42",
    "@types/lodash": "4.17.16",
    "@types/node": "18.19.76",
    "@types/debug": "4.1.12",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "gatsby": "5.14.6",
    "memfs": "4.36.0",
    "postcss": "8.4.38",
    "pkgroll": "2.15.3",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "semantic-release": "22.0.12",
    "typescript": "5.1.6",
    "vite": "6.2.0",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "gatsby": ">=5.10.0",
    "postcss": ">=8.4.38"
  },
  "engines": {
    "node": ">=14"
  },
  "renovate": {
    "extends": [
      "config:js-lib"
    ]
  }
}
